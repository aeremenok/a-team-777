<?xml version="1.0" encoding="utf-8"?>
<data-map project-version="2.0">
	<property name="defaultPackage" value="ru.spb.etu.server.model"/>
	<property name="defaultSchema" value="etu"/>
	<db-entity name="db_artist" schema="etu">
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="birthdate" type="TIMESTAMP"/>
		<db-attribute name="country" type="VARCHAR" length="10"/>
		<db-attribute name="description" type="VARCHAR" length="255"/>
		<db-attribute name="imageUrl" type="VARCHAR" length="255"/>
		<db-attribute name="name" type="VARCHAR" length="30"/>
	</db-entity>
	<db-entity name="db_genre" schema="etu">
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="Name" type="VARCHAR" length="255"/>
		<db-attribute name="descrption" type="VARCHAR" length="255"/>
		<db-attribute name="imageURL" type="VARCHAR" length="255"/>
	</db-entity>
	<db-entity name="db_masterpiece" schema="etu">
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="ID_artist" type="INTEGER" isMandatory="true"/>
		<db-attribute name="ID_genre" type="INTEGER" isMandatory="true"/>
		<db-attribute name="ID_museum" type="INTEGER"/>
		<db-attribute name="ID_type" type="INTEGER" isMandatory="true"/>
		<db-attribute name="creationYear" type="INTEGER"/>
		<db-attribute name="description" type="VARCHAR" length="255"/>
		<db-attribute name="imageURL" type="VARCHAR" length="255"/>
		<db-attribute name="photo" type="VARCHAR" length="255"/>
		<db-attribute name="title" type="VARCHAR" length="255"/>
	</db-entity>
	<db-entity name="db_museum" schema="etu">
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="description" type="VARCHAR" length="255"/>
		<db-attribute name="imageURL" type="VARCHAR" length="255"/>
		<db-attribute name="name" type="VARCHAR" length="50"/>
		<db-attribute name="www" type="VARCHAR" length="255"/>
	</db-entity>
	<db-entity name="db_painting" schema="etu">
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="height" type="INTEGER"/>
		<db-attribute name="width" type="INTEGER"/>
	</db-entity>
	<db-entity name="db_sculpture" schema="etu">
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="mass" type="INTEGER"/>
		<db-attribute name="material" type="VARCHAR" length="40"/>
	</db-entity>
	<obj-entity name="DbArtist" className="ru.spb.etu.server.model.DbArtist" dbEntityName="db_artist">
		<obj-attribute name="birthdate" type="java.util.Date" db-attribute-path="birthdate"/>
		<obj-attribute name="country" type="java.lang.String" db-attribute-path="country"/>
		<obj-attribute name="description" type="java.lang.String" db-attribute-path="description"/>
		<obj-attribute name="imageUrl" type="java.lang.String" db-attribute-path="imageUrl"/>
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="name"/>
	</obj-entity>
	<obj-entity name="DbGenre" className="ru.spb.etu.server.model.DbGenre" dbEntityName="db_genre">
		<obj-attribute name="descrption" type="java.lang.String" db-attribute-path="descrption"/>
		<obj-attribute name="imageURL" type="java.lang.String" db-attribute-path="imageURL"/>
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="Name"/>
	</obj-entity>
	<obj-entity name="DbMasterpiece" className="ru.spb.etu.server.model.DbMasterpiece" dbEntityName="db_masterpiece">
		<obj-attribute name="creationYear" type="java.lang.Integer" db-attribute-path="creationYear"/>
		<obj-attribute name="description" type="java.lang.String" db-attribute-path="description"/>
		<obj-attribute name="idType" type="java.lang.Integer" db-attribute-path="ID_type"/>
		<obj-attribute name="imageURL" type="java.lang.String" db-attribute-path="imageURL"/>
		<obj-attribute name="photo" type="java.lang.String" db-attribute-path="photo"/>
		<obj-attribute name="title" type="java.lang.String" db-attribute-path="title"/>
	</obj-entity>
	<obj-entity name="DbMuseum" className="ru.spb.etu.server.model.DbMuseum" dbEntityName="db_museum">
		<obj-attribute name="description" type="java.lang.String" db-attribute-path="description"/>
		<obj-attribute name="imageURL" type="java.lang.String" db-attribute-path="imageURL"/>
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="name"/>
		<obj-attribute name="www" type="java.lang.String" db-attribute-path="www"/>
	</obj-entity>
	<obj-entity name="DbPainting" superEntityName="DbMasterpiece" className="ru.spb.etu.server.model.DbPainting">
		<qualifier><![CDATA[ID_type = 1]]></qualifier>
		<obj-attribute name="height" type="java.lang.Integer"/>
		<obj-attribute name="width" type="java.lang.Integer"/>
	</obj-entity>
	<obj-entity name="DbSculpture" superEntityName="DbMasterpiece" className="ru.spb.etu.server.model.DbSculpture">
		<qualifier><![CDATA[ID_type = 2]]></qualifier>
		<obj-attribute name="mass" type="java.lang.Integer"/>
		<obj-attribute name="material" type="java.lang.String"/>
	</obj-entity>
	<db-relationship name="masterpieces" source="db_artist" target="db_masterpiece" toMany="true">
		<db-attribute-pair source="ID" target="ID_artist"/>
	</db-relationship>
	<db-relationship name="masterpieces" source="db_genre" target="db_masterpiece" toMany="true">
		<db-attribute-pair source="ID" target="ID_genre"/>
	</db-relationship>
	<db-relationship name="ispainting" source="db_masterpiece" target="db_painting" toDependentPK="true" toMany="false">
		<db-attribute-pair source="ID" target="ID"/>
	</db-relationship>
	<db-relationship name="issculpture" source="db_masterpiece" target="db_sculpture" toDependentPK="true" toMany="false">
		<db-attribute-pair source="ID" target="ID"/>
	</db-relationship>
	<db-relationship name="myartist" source="db_masterpiece" target="db_artist" toMany="false">
		<db-attribute-pair source="ID_artist" target="ID"/>
	</db-relationship>
	<db-relationship name="mygenre" source="db_masterpiece" target="db_genre" toMany="false">
		<db-attribute-pair source="ID_genre" target="ID"/>
	</db-relationship>
	<db-relationship name="mymuseum" source="db_masterpiece" target="db_museum" toMany="false">
		<db-attribute-pair source="ID_museum" target="ID"/>
	</db-relationship>
	<db-relationship name="masterpieces" source="db_museum" target="db_masterpiece" toMany="true">
		<db-attribute-pair source="ID" target="ID_museum"/>
	</db-relationship>
	<db-relationship name="generalmasterpiece" source="db_painting" target="db_masterpiece" toMany="false">
		<db-attribute-pair source="ID" target="ID"/>
	</db-relationship>
	<db-relationship name="mymasterpiece" source="db_sculpture" target="db_masterpiece" toMany="false">
		<db-attribute-pair source="ID" target="ID"/>
	</db-relationship>
	<obj-relationship name="masterpieces" source="DbArtist" target="DbMasterpiece" db-relationship-path="masterpieces"/>
	<obj-relationship name="masterpieces" source="DbGenre" target="DbMasterpiece" db-relationship-path="masterpieces"/>
	<obj-relationship name="ispainting" source="DbMasterpiece" target="DbPainting" db-relationship-path="ispainting"/>
	<obj-relationship name="issculpture" source="DbMasterpiece" target="DbSculpture" db-relationship-path="issculpture"/>
	<obj-relationship name="myartist" source="DbMasterpiece" target="DbArtist" db-relationship-path="myartist"/>
	<obj-relationship name="mygenre" source="DbMasterpiece" target="DbGenre" db-relationship-path="mygenre"/>
	<obj-relationship name="mymuseum" source="DbMasterpiece" target="DbMuseum" db-relationship-path="mymuseum"/>
	<obj-relationship name="masterpieces" source="DbMuseum" target="DbMasterpiece" db-relationship-path="masterpieces"/>
	<obj-relationship name="generalmasterpiece" source="DbPainting" target="DbMasterpiece"/>
	<obj-relationship name="mymasterpiece" source="DbSculpture" target="DbMasterpiece"/>
</data-map>
