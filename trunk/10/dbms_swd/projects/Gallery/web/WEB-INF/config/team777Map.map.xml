<?xml version="1.0" encoding="utf-8"?>
<data-map project-version="2.0">
	<property name="defaultPackage" value="ru.spb.etu.server.model"/>
	<property name="defaultSchema" value="etu"/>
	<db-entity name="db_artist" schema="etu">
		<db-attribute name="artistID" type="INTEGER" isPrimaryKey="true" isGenerated="true" isMandatory="true"/>
		<db-attribute name="birthdate" type="TIMESTAMP"/>
		<db-attribute name="country" type="VARCHAR" length="10"/>
		<db-attribute name="description" type="VARCHAR" length="255"/>
		<db-attribute name="imageUrl" type="VARCHAR" length="255"/>
		<db-attribute name="name" type="VARCHAR" length="30"/>
		<db-attribute name="picture" type="BLOB"/>
	</db-entity>
	<db-entity name="db_genre" schema="etu">
		<db-attribute name="Name" type="VARCHAR" length="255"/>
		<db-attribute name="descrption" type="VARCHAR" length="255"/>
		<db-attribute name="genreID" type="INTEGER" isPrimaryKey="true" isGenerated="true" isMandatory="true"/>
		<db-attribute name="imageURL" type="VARCHAR" length="255"/>
		<db-attribute name="picture" type="BLOB"/>
	</db-entity>
	<db-entity name="db_masterpiece" schema="etu">
		<db-attribute name="ID_artist" type="INTEGER" isMandatory="true"/>
		<db-attribute name="ID_genre" type="INTEGER"/>
		<db-attribute name="ID_museum" type="INTEGER"/>
		<db-attribute name="ID_type" type="INTEGER" isMandatory="true"/>
		<db-attribute name="creationYear" type="INTEGER"/>
		<db-attribute name="description" type="VARCHAR" length="255"/>
		<db-attribute name="imageURL" type="VARCHAR" length="255"/>
		<db-attribute name="masterpieceID" type="INTEGER" isPrimaryKey="true" isGenerated="true" isMandatory="true"/>
		<db-attribute name="photo" type="VARCHAR" length="255"/>
		<db-attribute name="picture" type="BLOB"/>
		<db-attribute name="title" type="VARCHAR" length="255"/>
	</db-entity>
	<db-entity name="db_museum" schema="etu">
		<db-attribute name="description" type="VARCHAR" length="255"/>
		<db-attribute name="imageURL" type="VARCHAR" length="255"/>
		<db-attribute name="museumID" type="INTEGER" isPrimaryKey="true" isGenerated="true" isMandatory="true"/>
		<db-attribute name="name" type="VARCHAR" length="50"/>
		<db-attribute name="picture" type="BLOB"/>
		<db-attribute name="www" type="VARCHAR" length="255"/>
	</db-entity>
	<db-entity name="db_painting" schema="etu">
		<db-attribute name="height" type="INTEGER"/>
		<db-attribute name="paintingID" type="INTEGER" isPrimaryKey="true" isGenerated="true" isMandatory="true"/>
		<db-attribute name="width" type="INTEGER"/>
	</db-entity>
	<db-entity name="db_sculpture" schema="etu">
		<db-attribute name="mass" type="INTEGER"/>
		<db-attribute name="material" type="VARCHAR" length="40"/>
		<db-attribute name="sculptureID" type="INTEGER" isPrimaryKey="true" isGenerated="true" isMandatory="true"/>
	</db-entity>
	<obj-entity name="DbArtist" className="ru.spb.etu.server.model.DbArtist" dbEntityName="db_artist">
		<obj-attribute name="birthdate" type="java.util.Date" db-attribute-path="birthdate"/>
		<obj-attribute name="country" type="java.lang.String" db-attribute-path="country"/>
		<obj-attribute name="description" type="java.lang.String" db-attribute-path="description"/>
		<obj-attribute name="imageUrl" type="java.lang.String" db-attribute-path="imageUrl"/>
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="name"/>
		<obj-attribute name="picture" type="byte[]" db-attribute-path="picture"/>
	</obj-entity>
	<obj-entity name="DbGenre" className="ru.spb.etu.server.model.DbGenre" dbEntityName="db_genre">
		<obj-attribute name="descrption" type="java.lang.String" db-attribute-path="descrption"/>
		<obj-attribute name="imageURL" type="java.lang.String" db-attribute-path="imageURL"/>
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="Name"/>
		<obj-attribute name="picture" type="byte[]" db-attribute-path="picture"/>
	</obj-entity>
	<obj-entity name="DbMasterpiece" className="ru.spb.etu.server.model.DbMasterpiece" dbEntityName="db_masterpiece">
		<qualifier><![CDATA[ID_type = 0]]></qualifier>
		<obj-attribute name="creationYear" type="java.lang.Integer" db-attribute-path="creationYear"/>
		<obj-attribute name="description" type="java.lang.String" db-attribute-path="description"/>
		<obj-attribute name="idType" type="java.lang.Integer" db-attribute-path="ID_type"/>
		<obj-attribute name="imageURL" type="java.lang.String" db-attribute-path="imageURL"/>
		<obj-attribute name="photo" type="java.lang.String" db-attribute-path="photo"/>
		<obj-attribute name="picture" type="byte[]" db-attribute-path="picture"/>
		<obj-attribute name="title" type="java.lang.String" db-attribute-path="title"/>
	</obj-entity>
	<obj-entity name="DbMuseum" className="ru.spb.etu.server.model.DbMuseum" dbEntityName="db_museum">
		<obj-attribute name="description" type="java.lang.String" db-attribute-path="description"/>
		<obj-attribute name="imageURL" type="java.lang.String" db-attribute-path="imageURL"/>
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="name"/>
		<obj-attribute name="picture" type="byte[]" db-attribute-path="picture"/>
		<obj-attribute name="www" type="java.lang.String" db-attribute-path="www"/>
	</obj-entity>
	<obj-entity name="DbPainting" superEntityName="DbMasterpiece" className="ru.spb.etu.server.model.DbPainting">
		<obj-attribute name="height" type="java.lang.Integer" db-attribute-path="height"/>
		<obj-attribute name="width" type="java.lang.Integer" db-attribute-path="width"/>
	</obj-entity>
	<obj-entity name="DbSculpture" superEntityName="DbMasterpiece" className="ru.spb.etu.server.model.DbSculpture">
		<obj-attribute name="mass" type="java.lang.Integer" db-attribute-path="mass"/>
		<obj-attribute name="material" type="java.lang.String" db-attribute-path="material"/>
	</obj-entity>
	<db-relationship name="artistMasterpiecedb" source="db_artist" target="db_masterpiece" toMany="true">
		<db-attribute-pair source="artistID" target="ID_artist"/>
	</db-relationship>
	<db-relationship name="genreMasterpiecedb" source="db_genre" target="db_masterpiece" toMany="true">
		<db-attribute-pair source="genreID" target="ID_genre"/>
	</db-relationship>
	<db-relationship name="myArtistdb" source="db_masterpiece" target="db_artist" toMany="false">
		<db-attribute-pair source="ID_artist" target="artistID"/>
	</db-relationship>
	<db-relationship name="myGenredb" source="db_masterpiece" target="db_genre" toMany="false">
		<db-attribute-pair source="ID_genre" target="genreID"/>
	</db-relationship>
	<db-relationship name="myMuseumdb" source="db_masterpiece" target="db_museum" toMany="false">
		<db-attribute-pair source="ID_museum" target="museumID"/>
	</db-relationship>
	<db-relationship name="museumMasterpiecedb" source="db_museum" target="db_masterpiece" toMany="true">
		<db-attribute-pair source="museumID" target="ID_museum"/>
	</db-relationship>
	<obj-relationship name="artistMasterpiece" source="DbArtist" target="DbMasterpiece" deleteRule="Cascade" db-relationship-path="artistMasterpiecedb"/>
	<obj-relationship name="genreMasterpiece" source="DbGenre" target="DbMasterpiece" deleteRule="Cascade" db-relationship-path="genreMasterpiecedb"/>
	<obj-relationship name="myArtist" source="DbMasterpiece" target="DbArtist" db-relationship-path="myArtistdb"/>
	<obj-relationship name="myGenre" source="DbMasterpiece" target="DbGenre" db-relationship-path="myGenredb"/>
	<obj-relationship name="myMuseum" source="DbMasterpiece" target="DbMuseum" db-relationship-path="myMuseumdb"/>
	<obj-relationship name="museumMasterpiece" source="DbMuseum" target="DbMasterpiece" deleteRule="Cascade" db-relationship-path="museumMasterpiecedb"/>
</data-map>
